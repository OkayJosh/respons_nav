	        				<svg width="120" height="120" viewBox="0 0 120 120" class="add">

	  							<defs>
	    							<pattern id="Triangle" width="12" height="12" patternUnits="userSpaceOnUse">
	      							<polygon points="5,0 10,10 0,10"/>
									</pattern>
	  							</defs>

	  						<circle cx="60" cy="60" r="50" fill="url(#Triangle)"/>
							</svg>

	        	 			<svg width="240" height="30" class="">

	  						<a href="https://github.com/okayjosh/" target="_blank">
	    						<rect height="30" width="220" y="0" x="0" rx="15"/>
	    						<text fill="white" text-anchor="middle" y="21" x="110">Olatunji Joshua Kayode</text>
							</a>
							</svg>
	        			</div>
	        	<div class=" nav add">
	        	        		
	       		</div>

	        </div>
	        {% endblock %}


	        {% block javascripts %}{% endblock %}
	    </body>
	</html>


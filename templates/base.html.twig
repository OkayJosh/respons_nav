	<!DOCTYPE html>
	<html>
	    <head>
	        <meta charset="UTF-8">
	        <title>{% block title %}Greetings- Olatunji Joshua Kayode{% endblock %}</title>
	        {% block stylesheets %}
            			<link rel="stylesheet" href="{{ asset('build/css/base.css') }}">
       		{% endblock %}
	    </head>
	    <body>
	    	{% block header%}
	    	<header class = "profile-box">
	    		<div>
	    			<img src='../build/image/me.jpg'>
	    		</div>
	    		
	    	</header>
	    	{% endblock %}

	        {% block body %}
	        	        <div class="nav">
	        	        	<div class=" nav add">
	        	        		
	        	        	</div>
